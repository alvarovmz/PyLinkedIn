{
    "classes": [
        {
            "name": "LinkedInUser",
            "url": [
                { "type": "constant", "value": "https://api.linkedin.com/v1/personal/~" }
            ],
            "attributes": [
                { "name": "firstName", "type": "string" },
                { "name": "lastName", "type": "string" },
                { "name": "maidenName", "type": "string" },
                { "name": "headline", "type": "string" },
                { "name": "industry", "type": "string" }
            ],
            "collections": [
                {    
                    "name": "skills",
                    "singularName": "skill",
                    "type": "Skill",
                    "url": [
                        { "type": "constant", "value": "http://api.linkedin.com/v1/people/~/skills" }
                    ],   
                    "getList": true 
                },
                {    
                    "name": "languages",
                    "singularName": "language",
                    "type": "Language",
                    "url": [
                        { "type": "constant", "value": "http://api.linkedin.com/v1/people/~/languages" }
                    ],   
                    "getList": true 
                },
                {    
                    "name": "connections",
                    "singularName": "connection",
                    "type": "LinkedInUser",
                    "url": [
                        { "type": "constant", "value": "http://api.linkedin.com/v1/people/~/connections" }
                    ],   
                    "getList": true 
                }
            ]
        },
        {
            "name": "Skill",
            "attributes": [
                { "name": "id", "type": "integer" },
                { "name": "skill", "type": "SkillDescription" }
            ]
        },
        {
            "name": "SkillDescription",
            "attributes": [
                { "name": "name", "type": "string" }
            ]
        },
        {
            "name": "Language",
            "attributes": [
                { "name": "id", "type": "integer" },
                { "name": "language", "type": "LanguageDescription" }
            ]
        },
        {
            "name": "LanguageDescription",
            "attributes": [
                { "name": "name", "type": "string" }
            ]
        }
    ]
}
